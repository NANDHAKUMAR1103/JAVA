class HelloWorld {
    
    public static void prime(int n) {
        
            for(int i=2;i<=n;i++){
                while(n%i==0){
                    System.out.print(i+" ");
                    n/=i;
                }
            }
        }
    
     
    public static void main(String[] args) {
        prime(50);
    }
}
